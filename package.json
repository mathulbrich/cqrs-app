{
  "name": "cqrs-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "prebuild:prod": "rimraf dist",
    "build": "nest build",
    "build:prod": "nest build --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "repl": "nest start --entryFile repl",
    "setup": "node dist/scripts/setup",
    "start": "nest start",
    "start:dev": "concurrently yarn:start:dev-*",
    "start:dev-app": "nest start --debug 0.0.0.0:9229 --watch",
    "start:dev-job": "nest start --entryFile src/queue --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch --maxWorkers=1",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.162.0",
    "@aws-sdk/client-s3": "^3.162.0",
    "@aws-sdk/client-sqs": "^3.162.0",
    "@aws-sdk/util-dynamodb": "^3.162.0",
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/event-emitter": "^1.3.1",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/serve-static": "^3.0.0",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "concurrently": "^7.3.0",
    "helmet": "^6.0.0",
    "lodash": "^4.17.21",
    "nestjs-pino": "^3.1.1",
    "pino": "^8.4.2",
    "pino-http": "^8.2.0",
    "pino-lambda": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "source-map-support": "^0.5.21",
    "validator": "^13.7.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.162.0",
    "@faker-js/faker": "^7.5.0",
    "@nestjs/cli": "^9.1.1",
    "@nestjs/schematics": "^9.0.2",
    "@nestjs/testing": "^9.0.11",
    "@types/async": "^3.2.15",
    "@types/aws-lambda": "^8.10.102",
    "@types/express": "^4.17.8",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.184",
    "@types/node": "^16.11.47",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "async": "^3.2.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "pino-pretty": "^9.1.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.7.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --color"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "coveragePathIgnorePatterns": [
      "src/bootstrap.ts",
      "src/lambda.events.ts",
      "src/lambda.ts",
      "src/main.ts",
      "src/queue.ts",
      "src/queue/lambda/sqs-event.ts",
      "src/queue/lambda/sqs-lambda.ts",
      "src/queue/managed/sqs-listener.ts",
      "src/standalone.module.ts"
    ],
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1",
      "^@test/(.*)$": "<rootDir>/test/$1"
    },
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^16.16.0"
  }
}
