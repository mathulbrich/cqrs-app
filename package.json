{
  "name": "cqrs-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "db:create": "./scripts/db-create.sh",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "repl": "nest start --entryFile repl",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.145.0",
    "@nestjs/common": "^9.0.8",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.8",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/mongoose": "^9.2.0",
    "@nestjs/platform-express": "^9.0.8",
    "@nestjs/serve-static": "^3.0.0",
    "@vendia/serverless-express": "^4.10.1",
    "aws-lambda": "^1.0.7",
    "body-parser": "^1.20.0",
    "helmet": "^5.1.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.2",
    "nestjs-pino": "^3.1.1",
    "pino": "^8.4.0",
    "pino-http": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "sqs-consumer": "^5.7.0",
    "validator": "^13.7.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.127.0",
    "@faker-js/faker": "^7.4.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.8",
    "@types/async": "^3.2.15",
    "@types/aws-lambda": "^8.10.102",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.8",
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.47",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "async": "^3.2.4",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "get-port": "^5.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "pino-pretty": "^9.0.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --color"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/app.controller.ts",
      "<rootDir>/src/bootstrap.ts",
      "<rootDir>/src/lambda.ts",
      "<rootDir>/src/main.ts",
      "<rootDir>/src/queue/application/sqs-queue-listener.ts"
    ],
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1",
      "^@test/(.*)$": "<rootDir>/test/$1"
    },
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^16.16.0"
  }
}
